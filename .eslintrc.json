{
	"extends": "eslint:recommended",
	"env": {
		"browser": true
	},
	"parserOptions": {
		"sourceType": "module",
		"ecmaVersion": 2020
	},
	"rules": {
		"indent": ["warn", "tab"],
		"quotes": ["warn", "double"],
		"semi": ["warn", "never"],
		"prefer-const": "warn",
		"no-var": "warn",
		"comma-dangle": ["warn", "always-multiline"],
		"space-before-function-paren": ["error", "always"],
		"max-len": ["warn", { "code": 128 }]
	}
}
